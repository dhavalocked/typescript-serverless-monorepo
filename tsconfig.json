{
  "compilerOptions": {
    "lib": ["es2017"],
    "removeComments": true,
    "module": "ES6",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "noUnusedParameters": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "target": "es2017",
    "outDir": "libs",
    "declaration": false,
    "listFiles": false
  }
}
